{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true
  }
  // Path aliases are handled by https://kit.svelte.dev/docs/configuration#alias. After adding an
  // alias, make sure to run "npm run dev" so that svelte kit can generate the appropriate tsconfig.
  //
  // If you want to overwrite includes/excludes, make sure to copy over the relevant
  // includes/excludes from the referenced tsconfig.json - TypeScript does not merge them in
}
